{
  "name": "mediacanvas",
  "version": "0.1.0",
  "description": "Render videos or images at multiple locations on a canvas simultaneously",
  "main": "index.js",
  "files": [
    "dist",
    "index.d.ts"
  ],
  "browserslist": [
    ">=1%",
    "not ie 11",
    "not op_mini all"
  ],
  "scripts": {
    "build": "webpack --config webpack.config.dev.js",
    "prepublishOnly": "webpack --config webpack.config.dev.js && webpack --config webpack.config.prod.js",
    "start": "webpack-dev-server --config webpack.config.dev.js --open"
  },
  "keywords": [
    "video",
    "image",
    "canvas",
    "render",
    "mosaic",
    "splitscreen",
    "split",
    "screen",
    "picture-in-picture",
    "pip"
  ],
  "author": "Aaron Mertzenich",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.7.0"
  }
}
